{
  "problemMatcher": [
    {
      "owner": "python-slow",
      "pattern": [
        {
          "regexp": "^=+ slowest durations =+$"
        },
        {
          "regexp": "^((.*s)\\s(call|setup|teardown)\\s+(.*)::(.*))$",
          "message": 1,
          "file": 2,
          "loop": true
        }
      ]
    }
  ]
}
